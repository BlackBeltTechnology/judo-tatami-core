@cached
operation JUDOPSM!TransferAttribute displayName() : String {
	return self.getTransferObjectType().name + "." + self.name;
}

@cached
operation JUDOPSM!TransferAttribute getTransferObjectType() : JUDOPSM!TransferObjectType {
	return JUDOPSM!TransferObjectType.all.selectOne(et | et.attributes.includes(self));
}

@cached
operation JUDOPSM!TransferAttribute isPrimitive() : Boolean {
	return self.dataType.isKindOf(JUDOPSM!Primitive);
}
