@cached
operation ASM!EPackage getFullName() : String {
	var package = ASM!EPackage.all.selectOne(p | p.eSubPackages.contains(self));
	var name = "";
	while (package.isDefined()) {
		name = package.name + "." + name;
		package = ASM!EPackage.all.selectOne(p | p.eSubPackages.contains(package));
	}

	return name + self.name;
}
