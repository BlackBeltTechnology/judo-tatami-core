@cached
operation ESM!Namespace toString() : String {
    var p : ESM!Namespace = self.getNamespace();
    if (p.isDefined()) {
        return p.toString() + "::" + self.name;
    } else {
        return self.name;
    }
}

@cached
operation ESM!NamespaceElement getNamespace() : ESM!Namespace {
    return ESM!Namespace.all.selectOne(p | p.elements.includes(self));
}