@cached
operation JQLEXTRACT!IntegerLiteral transformToExpression() : EXPR!Expression {
	return self.equivalent("IntegerLiteralToExpression");
} 

@cached
operation JQLEXTRACT!DecimalLiteral transformToExpression() : EXPR!Expression {
