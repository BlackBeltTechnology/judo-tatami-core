rule IntegerLiteralToExpression
  transform s : JQLEXTRACT!IntegerLiteral
  to t : EXPR!IntegerConstant {
  	t.value = s.value;
}

rule DecimalLiteralToExpression
  transform s : JQLEXTRACT!DecimalLiteral
  to t : EXPR!DecimalConstant {
  	t.value = s.value;
}

rule BooleanLiteralToExpression
