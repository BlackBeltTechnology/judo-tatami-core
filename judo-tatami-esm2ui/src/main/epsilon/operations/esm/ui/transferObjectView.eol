operation ESM!esm::ui::TransferObjectView createFlexFromTransferObjectView(application : UI!ui::Application) : UI!ui::Flex {
	var t : UI!ui::Flex = self.createFlexFromContainer(application);
	t.dataElement = self.eContainer.mapClassType(application);	
    return t;
}

operation ESM!esm::ui::TransferObjectView map(application : UI!ui::Application) : UI!ui::Flex {
	var t : UI!ui::Flex = self.createFlexFromTransferObjectView(application);
    return t;
}
