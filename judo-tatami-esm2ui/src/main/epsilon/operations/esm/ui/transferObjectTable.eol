operation ESM!esm::ui::TransferObjectTable createTableFromTransferObjectTable() : UI!ui::Table {
	var t : UI!ui::Table = new UI!ui::Table();
	t.name = self.name;
    t.binding = modelAdapter.buildTypeNameFromNamedElement(self).get();
	
	for (column in self.columns) {
		t.columns.add(column.map());
	}	
    return t;
}

operation ESM!esm::ui::TransferObjectTable map() : UI!ui::Table {
	var t : UI!ui::Table = self.createTableFromTransferObjectTable();
    return t;
}