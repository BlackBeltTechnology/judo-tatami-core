operation ESM!esm::ui::TabularReferenceField createTableFromTabularReferenceField() : UI!ui::Table {
	var table : UI!ui::Table = new UI!ui::Table();
    table.binding = modelAdapter.buildTypeNameFromNamedElement(self).get();
	table.name = self.name;	
	
	for (column in self.columns) {
		table.columns.add(column.map());
	}
	return table;
}

operation ESM!esm::ui::TabularReferenceField map() : UI!ui::Table {
	return self.createTableFromTabularReferenceField();
}
