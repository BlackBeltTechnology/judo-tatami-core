operation ESM!esm::ui::TextField createTextFromTextField() : UI!ui::Text {
	var ret : UI!ui::Text = new UI!ui::Text();
	ret.name = self.name;
	ret.label = self.text;
	ret.col = self.basis.asReal();
    ret.sourceId = self.getID();

	if (self.getEffectiveGrow()) {
        ret.stretch = true;
    } else {
        ret.stretch = false;
    }
	return ret;
}

operation ESM!esm::ui::TextField map(application : UI!ui::Application) : UI!ui::Text {
	return self.createTextFromTextField();
}
