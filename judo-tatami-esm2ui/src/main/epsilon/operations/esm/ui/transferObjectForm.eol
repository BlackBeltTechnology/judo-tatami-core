operation ESM!esm::ui::TransferObjectForm createFlexFromTransferObjectForm(prefix : String) : UI!ui::Flex {
    return self.transform(prefix);
}


operation ESM!esm::ui::TransferObjectForm transform(prefix : String) : UI!ui::Flex {
	var t : UI!ui::Flex = self.transformContainer(prefix);
    return t;
}