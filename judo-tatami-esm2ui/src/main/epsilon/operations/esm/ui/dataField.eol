operation ESM!esm::ui::DataField createInputFromDataField() : UI!ui::Input {
	var ret : UI!ui::Input = self.dataFeature.getMember().getDataType().map();
	ret.binding = self.getBindingName();
	ret.name = self.name;
	ret.label = self.label; //self.dataFeature.getMember().getDisplayLabel();
	return ret;
}

operation ESM!esm::ui::DataField map() : UI!ui::Input {
	return self.createInputFromDataField();
}
