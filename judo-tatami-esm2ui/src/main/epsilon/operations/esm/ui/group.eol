operation ESM!esm::ui::Group createFlexFromGroup(application : UI!ui::Application) : UI!ui::Flex {
	var ret : UI!ui::Flex = self.createFlexFromContainer(application);
	ret.sourceId = self.getID();
	return ret;
}

operation ESM!esm::ui::Group map(application : UI!ui::Application) : UI!ui::Flex {
	return self.createFlexFromGroup(application);
}
