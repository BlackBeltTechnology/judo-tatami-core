name: {{modelPackage name}}
description: UI for {{modelName name}} applications.

publish_to: 'none' # Remove this line if you wish to publish to pub.dev

version: 1.0.0+1

environment:
    sdk: ">=2.9.2 <2.12.0-0"

dependencies:
    flutter:
        sdk: flutter

    cupertino_icons: ^0.1.3
    # dynamic_widget: ^2.0.1
    dartz: ^0.9.1
    auto_size_text: ^2.1.0
    dio: '>=3.0.3 <4.0.0'
    openapi_dart_common: ^3.1.0
    mobx: ^1.2.1+1
    flutter_mobx: ^1.1.0+1
    provider: ^4.1.3

    openapi_generator_annotations: ^1.1.2
    get_it: ^4.0.4
    auto_route: ^0.6.7
    injectable: ^1.0.4
    universal_html: ^1.2.1
    universal_platform: ^0.1.3
    shared_preferences: ^0.5.10
    flutter_breadcrumb: ^0.1.2
    intl: ^0.16.1
    uuid: ^2.2.2
    quiver: ^2.1.3
    judo_flutter_components:
        git:
            url: git://github.com/BlackBeltTechnology/judo-flutter-components.git
            ref: master
    flutter_appauth_web:
        git:
            url: git://github.com/BlackBeltTechnology/flutter-appauth-web.git
            ref: master

dev_dependencies:
    flutter_test:
        sdk: flutter
    build_runner: ^1.10.0
    mobx_codegen: ^1.1.0+1
    openapi_generator: ^1.1.2
    auto_route_generator: ^0.6.7
    injectable_generator: ^1.0.4

dependency_overrides:
    analyzer: ^0.40.5
    openapi_generator_cli:
        git:
            url: git://github.com/BlackBeltTechnology/openapi-generator-dart.git
            path: openapi-generator-cli
            ref: addReservedWordsPathed

    openapi_generator:
        git:
            url: git://github.com/BlackBeltTechnology/openapi-generator-dart.git
            path: openapi-generator
            ref: addReservedWordsPathed

    openapi_generator_annotations:
        git:
            url: git://github.com/BlackBeltTechnology/openapi-generator-dart.git
            path: openapi-generator-annotations
            ref: addReservedWordsPathed

flutter:

    uses-material-design: true

    # To add assets to your application, add an assets section, like this:
    # assets:
    #   - images/a_dot_burr.jpeg
    #   - images/a_dot_ham.jpeg


    #
    # For details regarding fonts from package dependencies,
    # see https://flutter.dev/custom-fonts/#from-packages
    fonts:
#        - family: Roboto
#          fonts:
#            - asset: fonts/Roboto-Regular.ttf

        - family: SourceSansPro
          fonts:
              - asset: fonts/SourceSansPro-Regular.ttf
              - asset: fonts/SourceSansPro-Italic.ttf
                style: italic

              - asset: fonts/SourceSansPro-ExtraLight.ttf
                weight: 100
              - asset: fonts/SourceSansPro-ExtraLight.ttf
                weight: 200
              - asset: fonts/SourceSansPro-Light.ttf
                weight: 300
              - asset: fonts/SourceSansPro-Regular.ttf
                weight: 400
              - asset: fonts/SourceSansPro-SemiBold.ttf
                weight: 500
              - asset: fonts/SourceSansPro-SemiBold.ttf
                weight: 600
              - asset: fonts/SourceSansPro-Bold.ttf
                weight: 700
              - asset: fonts/SourceSansPro-Black.ttf
                weight: 800
              - asset: fonts/SourceSansPro-Black.ttf
                weight: 900

              - asset: fonts/SourceSansPro-ExtraLightItalic.ttf
                style: italic
                weight: 100
              - asset: fonts/SourceSansPro-ExtraLightItalic.ttf
                style: italic
                weight: 200
              - asset: fonts/SourceSansPro-LightItalic.ttf
                style: italic
                weight: 300
              - asset: fonts/SourceSansPro-Italic.ttf
                style: italic
                weight: 400
              - asset: fonts/SourceSansPro-SemiBoldItalic.ttf
                style: italic
                weight: 500
              - asset: fonts/SourceSansPro-SemiBoldItalic.ttf
                style: italic
                weight: 600
              - asset: fonts/SourceSansPro-BoldItalic.ttf
                style: italic
                weight: 700
              - asset: fonts/SourceSansPro-BlackItalic.ttf
                style: italic
                weight: 800
              - asset: fonts/SourceSansPro-BlackItalic.ttf
                style: italic
                weight: 900
