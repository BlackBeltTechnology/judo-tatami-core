part '{{importName}}_store.g.dart';

class NorthwindExternalAPCategoryInfoStore
    extends _NorthwindExternalAPCategoryInfoStore
    with _$NorthwindExternalAPCategoryInfoStore {
  NorthwindExternalAPCategoryInfoStore() : super();

  NorthwindExternalAPCategoryInfoStore.clone(
      NorthwindExternalAPCategoryInfoStore categoryInfoStore)
      : super.clone(categoryInfoStore);
}

abstract class _NorthwindExternalAPCategoryInfoStore
    with Store, NorthwindExternalAPCategoryInfoRepository {
  _NorthwindExternalAPCategoryInfoStore();

  _NorthwindExternalAPCategoryInfoStore.clone(
      NorthwindExternalAPCategoryInfoStore categoryInfoStore) {
    identifier = categoryInfoStore.identifier;
    categoryName = categoryInfoStore.categoryName;
    products = ObservableList.of(categoryInfoStore.products);
  }

  String identifier;

  @observable
  String categoryName;

  @observable
  var products = ObservableList<NorthwindExternalAPProductInfoStore>();
}
