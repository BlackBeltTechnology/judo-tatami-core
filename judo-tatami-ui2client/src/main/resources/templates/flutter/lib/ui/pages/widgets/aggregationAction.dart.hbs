{{ log 'Aggregation action button generation for' (variable application.name) '-' (fqClass page.name)'page'(variable relation.name)'relation' level='debug' ~}}
{{# if addDebugToTemplate }}//START aggregationAction.dart.hbs-----{{ relation.name }}-----------------------------------------------------------------------------------{{/ if }}
Observer(
    builder: (_) => JudoButton(
        col: {{ col }},
        {{# neq row 1.0 }}row: {{ row }},{{/ neq }}
        {{# if disabled }}
            disabled: true,
        {{ else }}
            disabled: pageStore.pageState.disabledByLoading{{# if enabledBy }} || !((targetStore?.{{ variable enabledBy.name }}) ?? false){{/ if }},
        {{/if}}
        {{# if action.isBackAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/backaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if action.isCreateAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/createaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if action.isAddAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/addaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if action.isSetAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/setaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if action.isUnsetAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/unsetaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if action.isNavigationToPageAction }}
            {{> flutter/lib/ui/pages/widgets/aggregationaction/navigationtopageaction.dart.hbs action=action page=page relation=relation }}
        {{/ if }}

        {{# if this.isLabelVisible }}
            label: AppLocalizations.of(context).lookUpValue(context, '{{ this.label }}'),
        {{/ if }}

        {{# if this.isIconVisible }}
            icon: Icon(getIconByString('{{ this.icon.name }}')),
        {{/ if }}
    )
),
{{# if addDebugToTemplate }}//END aggregationAction.dart.hbs-----{{ relation.name }}-------------------------------------------------------------------------------------{{/ if }}
