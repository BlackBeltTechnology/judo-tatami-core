{{~ log 'Enumeration combo generation for' (variable application.name) '-' (fqClass page.name)'page' label level='debug' }}
{{# if addDebugToTemplate }}//START xmi:id="{{ getXMIID this }}" enumerationcombo.dart.hbs-----{{ page.name }}----------------------------------------------------------------------------------------{{/ if }}
Observer(
    builder: (_) => JudoComboBox<{{ className attributeType.dataType.name }}>(
        key: {{# if (isViewTypePage page) }}UniqueKey(){{ else }}inputWidgetKeyMap['{{ name }}']{{/ if }},
        {{# if (isPageValidatable page) }}
        errorMessage: pageStore.validationAttributeErrorMessageMap['{{ variable attributeType.name }}'].message,
        {{/ if }}
        col: {{ col }},
        {{# neq row 1.0 }}row: {{ row }},{{/ neq }}
        {{# if this.isIconVisible }}
        icon: Icon(getIconByString('{{ this.icon.name }}')),
        {{/ if }}
        {{# if this.isLabelVisible }}
        label: AppLocalizations.of(context).lookUpValue(context, '{{ label }}'),
        {{/ if }}
        {{# if isInCard }}inCard: true,{{/ if }}
        {{# if disabled }}
            disabled: true,
        {{ else if enabledBy }}
            disabled: !((targetStore?.{{ variable enabledBy.name }}) ?? false),
        {{/ if }}
        items: {{ className attributeType.dataType.name }}.values,
        value: targetStore.{{ variable attributeType.name }},
        {{# unless (isViewTypePage page) }}
        onChanged: (value) => targetStore.set{{ className attributeType.name }}(value),
        {{/ unless }}
        dropdownMenuShow: (value) => new DropdownMenuItem<{{ className attributeType.dataType.name }}>(
            value: value,
            child: new Text(AppLocalizations.of(context).lookUpValue(context, value.toString().split('.').last))
        ),
        readOnly: {{# if (isViewTypePage page) }}true{{ else }}{{ attributeType.isReadOnly }}{{/ if }},
    )
),
{{# if addDebugToTemplate }}//END enumerationcombo.dart.hbs-----{{ page.name }}------------------------------------------------------------------------------------------{{/ if }}
