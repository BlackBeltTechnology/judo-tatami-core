{{~ log 'Tab generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
//START tabcontroller.dart.hbs-----{{name}}------------------------------------------------------------------------------------------------
JudoTab(
    col: {{ col }},
    row: {{ row }},
    tabContent: [
    {{# each tabs as |tab| }}
        {{# if tab }}
            {{# with tab.element as |child|}}
                {{> (getWidgetTemplate child) }}
            {{/ with }}
        {{/ if }}
    {{/ each }}
    ],
    tabs: [
    {{# each tabs as |tab| }}
        Tab(
        {{#if tab.element.isIconVisible }}
            icon: Icon(Icons.{{ tab.element.icon.name }}),
        {{/if}}
        {{#if tab.element.isLabelVisible }}
            text: AppLocalizations.of(context).lookUpValue(context, '{{ tab.element.label }}'),
        {{/if}}
        ),
    {{/ each }}
    ]
),
//END tabcontroller.dart.hbs-----{{name}}--------------------------------------------------------------------------------------------------
