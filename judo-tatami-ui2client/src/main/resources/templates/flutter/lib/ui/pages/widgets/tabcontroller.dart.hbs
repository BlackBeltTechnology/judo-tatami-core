{{~ log 'Tab generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
JudoTab(
    col: {{ multiplyCol col }},
    row: {{ row }},
    tabContent: [
    {{# each tabs as |tab| }}
        {{# if tab }}
        {{> (getWidgetTemplate tab) }}
        {{/ if }}
    {{/ each }}
    ]
    tab: [
    {{# each tabs as |tab| }}
        Tab(
        {{#if tab.icon }}
            icon: Icon(Icons.{{ tab.icon.name }}),
        {{/if}}
            text: {{ tab.label }},
        ),
    {{/ each }}
    ]
),