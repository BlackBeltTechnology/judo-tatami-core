{{~ log 'Tab generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
JudoTab(
    col: {{ multiplyCol col }},
    row: {{ row }},
    tabContent: [
    {{# each tabs as |tab| }}
        {{# if tab }}
            {{# with tab.element as |child|}}
                {{> (getWidgetTemplate child) }}
            {{/ with }}
        {{/ if }}
    {{/ each }}
    ],
    tabs: [
    {{# each tabs as |tab| }}
        Tab(
        {{#if tab.icon }}
            icon: Icon(Icons.{{ tab.icon.name }}),
        {{/if}}
            text: "{{ tab.element.label }}",
        ),
    {{/ each }}
    ]
),