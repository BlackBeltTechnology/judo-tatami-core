JudoButton(
    col: {{ col }},
{{# if this.action.isNavigationToPageAction }}
    onPressed: () async {
        await navigation.open(Routes.{{ fqVariable this.action.target.name }}Page,
        arguments: {{ fqClass this.action.target.name }}PageArguments(target{{ className page.dataElement.target.name }}Store: target{{ className page.dataElement.target.name }}Store));
    },
    {{#if icon }}
    icon: Icon(Icons.{{icon.name}}),
    {{/if}}
{{/ if }}
{{# if this.action.isSaveAction }}
    onPressed: () => navigation.close(target{{ className page.dataElement.target.name }}Store),
    {{#if icon }}
    icon: Icon(Icons.{{icon.name}}),
    {{/if}}
{{/ if }}
{{# if this.action.isBackAction }}
    onPressed: () => navigation.close(),
    {{#if icon}}
    icon: Icon(Icons.{{icon.name}}),
    {{/if}}
{{/ if }}
    label: '{{ label }}',
    rounded: true,
),
