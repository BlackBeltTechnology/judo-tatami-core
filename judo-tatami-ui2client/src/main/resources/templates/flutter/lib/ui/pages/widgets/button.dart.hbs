JudoButton(
    col: {{ col }},
{{# if (isSaveButton this.action) }}
    onPressed: () => navigation.close(target{{ className page.targetClassType.name }}Store),
    {{#if icon }}
    icon: Icon(Icons.{{icon.name}}),
    {{/if}}
{{/ if }}
{{# if (isBackButton this.action) }}
    onPressed: () => navigation.close(),
    {{#if icon}}
    icon: Icon(Icons.{{icon.name}}),
    {{/if}}
{{/ if }}
    label: '{{ label }}',
    rounded: true,
),
