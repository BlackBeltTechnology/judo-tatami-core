onPressed: () async {
    {{# if page.isPageTypeUpdate }}
        {{> flutter/lib/ui/pages/widgets/instanceaction/saveaction/update.dart.hbs navigation=navigation action=action page=page }}
    {{/ if }}
    {{# if page.isPageTypeCreate }}
        {{> flutter/lib/ui/pages/widgets/instanceaction/saveaction/create.dart.hbs navigation=navigation action=action page=page }}
    {{/ if }}
    {{# if page.isPageTypeOperationInput }}
        {{> flutter/lib/ui/pages/widgets/instanceaction/saveaction/operationinput.dart.hbs navigation=navigation action=action page=page }}
    {{/ if }}
},
