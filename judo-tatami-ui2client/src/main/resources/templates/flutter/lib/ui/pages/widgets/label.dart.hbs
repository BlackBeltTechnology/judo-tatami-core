{{ log 'Label generation for' (variable application.name) '-' (fqClass page.name)'page' label level='debug' ~}}
//START xmi:id="{{ getXMIID this }}" label.dart.hbs-----{{ page.name }}---------------------------------------------------------------------------------------------------
JudoLabel(
    col: {{ col }},
    row: {{ row }},
  {{# if isLabelVisible }}
    label: AppLocalizations.of(context).lookUpValue(context, '{{ label }}'),
    labelMargin: {{ labelMargin.left }},
  {{/ if }}
  {{# if isIconVisible }}
    icon: Icon(getIconByString("{{ icon.name }}")),
    iconMargin: {{ iconMargin.right }},
  {{/ if }}
),
//END label.dart.hbs-----{{ page.name }}-----------------------------------------------------------------------------------------------------
