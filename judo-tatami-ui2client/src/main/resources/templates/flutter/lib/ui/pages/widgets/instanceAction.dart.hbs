{{ log 'Instance action button generation for' (variable application.name) '-' (fqClass page.name)'page'(variable page.dataElement.target.name) level='debug' ~}}
{{# if addDebugToTemplate }}//START instanceAction.dart.hbs-----{{ page.dataElement.target.name }}-----------------------------------------------------------------------{{/ if }}
Observer(
    builder: (_) => JudoButton(
        {{# if (isActionNotNavigateToPage action) }}
        loadingState: pageStore.{{ variable button.name }}LoadingState,
        {{/ if }}
        {{# if action.isFilterAction }}
        key: addFilterButtonKey,
        {{/ if }}
        {{# neq row 1.0 }}row: {{ row }},{{/ neq }}
        col: {{ col }},
        disabled: pageStore.pageState.disabledByLoading || {{# if action.isBackAction }}navigation.stack.length == 0{{ else }}{{ disabled }}{{# if enabledBy }} || !((targetStore?.{{ variable enabledBy.name }}) ?? false){{/ if }}{{/ if }},
    {{# unless disabled }}
        {{# if action.isSaveAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/saveaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isBackAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/backaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isRefreshAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/refreshaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isFilterAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/filteraction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isCreateAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/createaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isDeleteAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/deleteaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isEditAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/editaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isAddAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/addaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isSetAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/setaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isUnsetAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/unsetaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isNavigationToPageAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/navigationtopageaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

        {{# if action.isCallOperationAction }}
            {{> flutter/lib/ui/pages/widgets/instanceaction/calloperationaction.dart.hbs navigation=navigation action=action page=page }}
        {{/ if }}

    {{/ unless }}
    {{# if this.isLabelVisible }}
        label: AppLocalizations.of(context).lookUpValue(context, '{{ this.label }}'),
    {{/ if }}
    {{# if this.isIconVisible }}
        icon: Icon(getIconByString('{{ this.icon.name }}')),
    {{/ if }}
    )
),
{{# if addDebugToTemplate }}//END instanceAction.dart.hbs-----{{ page.dataElement.target.name }}-------------------------------------------------------------------------{{/ if }}
