{{ log 'Switch generation for' (variable application.name) '-' (fqClass page.name)'page' label level='debug' ~}}
{{# if addDebugToTemplate }}//START xmi:id="{{ getXMIID this }}" switch.dart.hbs-----{{ page.name }}----------------------------------------------------------------------------------------------------{{/ if }}
Observer(
    builder: (_) => JudoSwitch(
        col: {{ col }},
        {{# neq row 1.0 }}row: {{ row }},{{/ neq }}
        {{# if (isPageValidatable page) }}
        errorMessage: pageStore.validationAttributeErrorMessageMap['{{ variable attributeType.name }}'].message,
        {{/ if }}
        {{# if disabled }}
            disabled: true,
        {{ else if enabledBy }}
            disabled: !((targetStore?.{{ variable enabledBy.name }}) ?? false),
        {{/ if }}
        {{# if this.isLabelVisible }}
        label: AppLocalizations.of(context).lookUpValue(context, '{{ this.label }}'),
        {{/ if }}
        {{# if this.isIconVisible }}
        icon: Icon(getIconByString('{{ this.icon.name }}')),
        {{/ if }}
        initialValue: targetStore.{{ variable attributeType.name }},
        {{# if attributeType.isReadOnly }}readOnly: true,{{/ if }}
        {{# unless (isViewTypePage page) }}
        {{# if attributeType.isRequired }}mandatory: true,{{/ if }}
        onChanged: (value) => targetStore.set{{ className attributeType.name }}(value),
        {{/ unless }}
    ),
),
{{# if addDebugToTemplate }}//END switch.dart.hbs-----{{ page.name }}----------------------------------------------------------------------------------------------------{{/ if }}
