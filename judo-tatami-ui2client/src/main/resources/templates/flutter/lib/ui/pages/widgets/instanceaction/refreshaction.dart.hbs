{{# if page.dataElement.isRelationBehaviourTypeRefreshable }}
    {{# if (isRefreshViewTypePage page) }}
        onPressed: () async {
        await pageStore.refresh{{ className page.relationType.target.name }}(targetStore);
        },
    {{/ if }}
    {{# if (isRefreshTableTypePage page) }}
        onPressed: () async {
        await pageStore.get{{ className page.relationType.name }}({{# unless page.dataElement.isAccess }}ownerStore{{/ unless }});
        },
    {{/ if }}
{{/ if }}
