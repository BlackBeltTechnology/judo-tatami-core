{{~ log 'Flex generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
{{# if isDirectionHorizontal }}
JudoRow(
{{ else }}
JudoColumn(
{{/ if }}
    col: {{ col }},
    row: {{ row }},
    mainAxisAlignment: MainAxisAlignment.{{ mainAxisAlignment this }},
    crossAxisAlignment: CrossAxisAlignment.{{ crossAxisAlignment this }},
    mainAxisSize: MainAxisSize.{{ mainAxisSize this }},
    children: [
    {{# each children as |child| }}
        {{# if child }}
        {{> (getWidgetTemplate child) }}
        {{/ if }}
    {{/ each }}
    ],
),
