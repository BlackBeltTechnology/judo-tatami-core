{{~ log 'Flex generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
{{# if addDebugToTemplate }}//START xmi:id="{{ getXMIID this }}" -----flex.dart.hbs-----{{ page.name }}----------------------------------------------------------------------------------------------------{{/ if }}
{{# if isDirectionHorizontal }}
JudoRow(
        {{# neq (mainAxisAlignment this) 'center' }}mainAxisAlignment: MainAxisAlignment.{{ mainAxisAlignment this }},{{/ neq }}
        {{# neq (crossAxisAlignment this) 'start' }}crossAxisAlignment: CrossAxisAlignment.{{ crossAxisAlignment this }},{{/ neq }}
{{ else }}
{{# if (isRefreshTableTypePage page) }}Observer(
    builder: (_) => {{/ if }}JudoColumn(
        {{# neq (mainAxisAlignment this) 'start' }}mainAxisAlignment: MainAxisAlignment.{{ mainAxisAlignment this }},{{/ neq }}
        {{# neq (crossAxisAlignment this) 'center' }}crossAxisAlignment: CrossAxisAlignment.{{ crossAxisAlignment this }},{{/ neq }}
{{/ if }}
        {{# neq (mainAxisSize this) 'min' }}mainAxisSize: MainAxisSize.{{ mainAxisSize this }},{{/ neq }}
        col: {{ col }},
        row: {{ row }}{{# unless isDirectionHorizontal }}{{# if (isRefreshTableTypePage page) }} + pageStore.getPlusRowSize{{/ if }}{{/ unless }},
        {{# if card }}card: true,{{/ if }}
    {{# if frame }}
        {{# neq frame.elevation 4.0 }}elevation: {{ frame.elevation }},{{/ neq }}
        {{# neq frame.radius 10.0 }}radius: {{ frame.radius }},{{/ neq }}
        {{# if frame.margin }}
        frameMargin: EdgeInsets.only(top: {{ frame.margin.top }}, bottom: {{ frame.margin.bottom }}, left: {{ frame.margin.left }}, right: {{ frame.margin.right }}),
        {{/ if }}
    {{/ if }}
        children: [
        {{# each children as |child| }}
            {{# if child }}
            {{> (getWidgetTemplate child) }}
            {{/ if }}
        {{/ each }}
        ],
    ),{{# unless isDirectionHorizontal }}{{# if (isRefreshTableTypePage page) }}
),{{/ if }}{{/ unless }}
{{# if addDebugToTemplate }}//END flex.dart.hbs-----{{ page.name }}------------------------------------------------------------------------------------------------------{{/ if }}
