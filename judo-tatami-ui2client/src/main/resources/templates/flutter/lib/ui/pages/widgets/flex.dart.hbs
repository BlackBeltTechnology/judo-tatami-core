{{~ log 'Flex generation for' (variable application.name) '-' (fqClass page.name)'page' (variable name) level='debug' ~}}
//START flex.dart.hbs-----{{page.name}}----------------------------------------------------------------------------------------------------
{{# if isDirectionHorizontal }}
JudoRow(
{{ else }}
{{# if (isRefreshTableTypePage page) }}Observer(
    builder: (_) => {{/ if }}JudoColumn(
{{/ if }}
        col: {{ multiplyCol col }},
        row: {{ row }}{{# unless isDirectionHorizontal }}{{# if (isRefreshTableTypePage page) }} + {{ fqVariable page.name }}PageStore.getPlusRowSize + 1{{/ if }}{{/ unless }},
        card: {{ frame }},
        mainAxisAlignment: MainAxisAlignment.{{ mainAxisAlignment this }},
        crossAxisAlignment: CrossAxisAlignment.{{ crossAxisAlignment this }},
        mainAxisSize: MainAxisSize.{{ mainAxisSize this }},
        children: [
        {{# each children as |child| }}
            {{# if child }}
            {{> (getWidgetTemplate child) }}
            {{/ if }}
        {{/ each }}
        ],
    ),{{# unless isDirectionHorizontal }}{{# if (isRefreshTableTypePage page) }}
),{{/ if }}{{/ unless }}
//END flex.dart.hbs-----{{page.name}}------------------------------------------------------------------------------------------------------
