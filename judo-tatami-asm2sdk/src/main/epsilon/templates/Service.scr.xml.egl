[%
  import "../operations/sdk.eol";
%]
<?xml version="1.0" encoding="UTF-8"?>
<component name="[%= packagePath %].[%= className %]" immediate="true">
  <implementation class="[%= packagePath %].[%= className %]"/>
  <service>
    [% for(op in operations) { %]
    <provide interface="[%= packagePath %].[%= className + "_" + op.name %]"/>
    [% } %]
  </service>
</component>
